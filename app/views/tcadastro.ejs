<!doctype html>
<html lang="pt-br">

<head>
  <meta charset="utf-8">
  <title>Requerimento Online</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="css/cadastro.css">
</head>

<body>
  <div class="cadastro">
    <header class="cadastro_header">
      <h1 class="titulo">ÁREA DE CADASTRO</h1>
    </header>
    <form class="form" action="/alunos/salvar" method="post">
      <div>
        <input type="text" placeholder="Nome Completo" class="form_input" id="nome" name="nome">
      </div>
      <div>
        <input type="text" placeholder="RA" class="form_input" id="ra" name="ra">
      </div>
      <div>
        <input type="text" placeholder="CPF" class="form_input" id="cpf" name="cpf">
      </div>
      <div>
        <input type="text" placeholder="RG" class="form_input" id="rg" name="rg">
      </div>
      <div>
        <input type="text" placeholder="Período" class="form_input" id="periodo" name="periodo">
      </div>
      <div>
        <input type="email" placeholder="E-mail" class="form_input" id="email" name="email" required>
      </div>
      <div>
        <input type="password" placeholder="Senha" class="form_input" id="senha" name="senha" required>
      </div>
      <div>
        <input type="password" placeholder="Confirme sua senha" class="form_input" id="c_senha" required>
      </div>
      <button class="btn" type="button">CADASTRAR</button>
    </form>
  </div>
  <div class="logo">
    <a href="index.html"><img src="img/logoif.png" alt='some text' width=50px height=66px></a>
  </div>
  <script src="js/cadastro.js"></script>
</body>

</html>
