<form class="form" action="/servidor/salvar" method="post">
    <div>
      <input type="text" placeholder="Nome Completo" class="form_input" id="nome" name="nome_servidor">
    </div>
    <div>
      <input type="text" placeholder="SIAPE" class="form_input" id="siape" name="siape_servidor">
    </div>
    <div>
      <input type="password" placeholder="Senha" class="form_input" id="senha" name="senha_servidor" required>
    </div>
    <div>
      <input type="password" placeholder="Confirme sua senha" class="form_input" id="csenha_senha" name="csenha_servidor" required>
    </div>
    <button class="btn" type="submit">CADASTRAR NOVO SERVIDOR</button>
</form>